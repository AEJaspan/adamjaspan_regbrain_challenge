    "import nltk\r\n",
    "def remove_non_words(string):\r\n",
    "    for esc_char in ['-','_','|']:\r\n",
    "        string = string.replace(esc_char, ' ')\r\n",
    "    words = set(nltk.corpus.words.words())\r\n",
    "    return \" \".join(w for w in nltk.wordpunct_tokenize(string) if w.lower() in words)"
   ],
